SELECT * FROM Estilistas WHERE experiencia >= 8 ;

SELECT * FROM confeccao;

SELECT DISTINCT cores, recortes FROM confeccao;

SELECT modelo, preco FROM Roupas ORDER BY preco DESC;

SELECT modelo, FK_Estilista_codigo as Quantidade_de_roupas_vendidas FROM Roupas;

SELECT * FROM Clientes;

SELECT nome, avaliacoes FROM Clientes WHERE avaliacoes IS NOT NULL;

SELECT * FROM Eventos;

SELECT nome AS Nome_Evento, localizacao AS Localizacao_Evento FROM Eventos WHERE localizacao IS NOT NULL;

SELECT FK_Cliente_email AS Email_Cliente FROM RoupasClientes;

SELECT FK_Eventos_codigo AS Codigo_Evento FROM EventosEstilistas;
